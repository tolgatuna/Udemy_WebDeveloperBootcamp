<h1>Results Page</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Year</th>
            <th>Type</th>
        </tr>
    </thead>
    <tbody>
        <% res.Search.forEach(function (entity) { %>
            <tr>
                <td><%= entity.Title%></td>
                <td><%= entity.Year%></td>
                <td><%= entity.Type%></td>
            </tr>
        <% }) %>
    </tbody>
</table>
<br>
<a href="/">Search Again?</a>